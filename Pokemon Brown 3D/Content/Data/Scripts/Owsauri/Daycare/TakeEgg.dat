version=2
	@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_WantEgg)>)
	@options.show(<system.token(global_yes)>,<system.token(global_no)>)
	:when:<system.token(global_yes)>
		:if:<pokemon.count><6
		:then
			@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_ReceivedEgg)>)
			@sound.play(Receive_Egg,1)
			@daycare.takeegg(0)
			@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_TakeCare)>)
		:else
			@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_NoRoom)>)
		:endif
	:when:<system.token(global_no)>
		@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_YouSure)>)
		@options.show(<system.token(global_yes)>,<system.token(global_no)>)
		:when:<system.token(global_yes)>
			:if:<pokemon.count><6
			:then
				@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_ReceivedEgg)>)
				@sound.play(Receive_Egg,1)
				@daycare.takeegg(0)
				@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_TakeCare)>)
			:else
				@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_NoRoom)>)
			:endif
		:when:<system.token(global_no)>
			@Text.Show(<system.token(Owsauri_Daycare_DaycareMan_TakeEgg_DoNotWantEgg)>)
			@daycare.removeegg(0)
		:endwhen
	:endwhen
:end
