version=2
:if:<Register.Registered(GameCorner1F_GamblingClerk)>=false
	@Text.Show(<system.token(Owsauri_GameCorner1F_GamblingClerk_1)>)
	:if:<inventory.countitem(54)>=1
		@sound.play(success_small)
		@Text.Show(<system.token(Owsauri_GameCorner1F_GamblingClerk_2)>)
		@player.addcoins(20)
		@Register.Register(GameCorner1F_GamblingClerk)
		@npc.turn(10,1)
		:end
	:else
		@Text.Show(<system.token(Owsauri_GameCorner1F_ForgotCoinCase)>)
		@npc.turn(10,1)
		:end
	:endif
:else
	@Text.Show(<system.token(Owsauri_GameCorner1F_GamblingClerk_3)>)
	@npc.turn(10,1)
	:end
:endif

:end