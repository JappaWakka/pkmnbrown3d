version=2
@storage.clear
@storage.set(int,AddItemAmount,0)
@storage.set(int,RemoveItemAmount,0)
@storage.set(str,AddItemOptions,<system.token(global_back)>)
@storage.set(str,RemoveItemOptions,<system.token(global_back)>)

:if:<Register.Registered(DecorationItem_SniffCat)>=true
	:if:<Register.Registered(DecorationPlaced_SniffCat)>=false
		@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Item_SniffCat)>,<storage.get(str,AddItemOptions)>)
		@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
	:else
		@storage.set(str,RemoveItemOptions,<system.token(PC_Decorations_Item_SniffCat)>,<storage.get(str,RemoveItemOptions)>)
		@storage.set(int,RemoveItemAmount,<storage.get(int,RemoveItemAmount)>+1)
	:endif
:endif
:if:<Register.Registered(DecorationItem_DJSet)>=true
	:if:<Register.Registered(DecorationPlaced_DJSet)>=false
		@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Item_DJSet)>,<storage.get(str,AddItemOptions)>)
		@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
	:else
		@storage.set(str,RemoveItemOptions,<system.token(PC_Decorations_Item_DJSet)>,<storage.get(str,RemoveItemOptions)>)
		@storage.set(int,RemoveItemAmount,<storage.get(int,RemoveItemAmount)>+1)
	:endif
:endif
:if:<Register.Registered(DecorationItem_LeagueTrophy)>=true
	:if:<Register.Registered(DecorationPlaced_LeagueTrophy)>=false
		@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Item_LeagueTrophy)>,<storage.get(str,AddItemOptions)>)
		@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
	:else
		@storage.set(str,RemoveItemOptions,<system.token(PC_Decorations_Item_LeagueTrophy)>,<storage.get(str,RemoveItemOptions)>)
		@storage.set(int,RemoveItemAmount,<storage.get(int,RemoveItemAmount)>+1)
	:endif
:endif
:if:<Register.Registered(DecorationItem_DexDiploma)>=true
	:if:<Register.Registered(DecorationPlaced_DexDiploma)>=false
		@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Item_DexDiploma)>,<storage.get(str,AddItemOptions)>)
		@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
	:else
		@storage.set(str,RemoveItemOptions,<system.token(PC_Decorations_Item_DexDiploma)>,<storage.get(str,RemoveItemOptions)>)
		@storage.set(int,RemoveItemAmount,<storage.get(int,RemoveItemAmount)>+1)
	:endif
:endif
:if:<Register.Registered(DecorationItem_SurfingPikachuDoll)>=true
	:if:<Register.Registered(DecorationPlaced_SurfingPikachuDoll)>=false
		@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Item_SurfingPikachuDoll)>,<storage.get(str,AddItemOptions)>)
		@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
	:else
		@storage.set(str,RemoveItemOptions,<system.token(PC_Decorations_Item_SurfingPikachuDoll)>,<storage.get(str,RemoveItemOptions)>)
		@storage.set(int,RemoveItemAmount,<storage.get(int,RemoveItemAmount)>+1)
	:endif
:endif
:if:<Register.Registered(DecorationItem_PottedPlant)>=true
	:if:<Register.Registered(DecorationPlaced_PottedPlant)>=false
		@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Item_PottedPlant)>,<storage.get(str,AddItemOptions)>)
		@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
	:else
		@storage.set(str,RemoveItemOptions,<system.token(PC_Decorations_Item_PottedPlant)>,<storage.get(str,RemoveItemOptions)>)
		@storage.set(int,RemoveItemAmount,<storage.get(int,RemoveItemAmount)>+1)
	:endif
:endif
:if:<Register.Registered(DecorationItem_Squirtle)>=true
	:if:<Register.Registered(DecorationPlaced_Squirtle)>=false
		@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Item_Squirtle)>,<storage.get(str,AddItemOptions)>)
		@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
	:else
		@storage.set(str,RemoveItemOptions,<system.token(PC_Decorations_Item_Squirtle)>,<storage.get(str,RemoveItemOptions)>)
		@storage.set(int,RemoveItemAmount,<storage.get(int,RemoveItemAmount)>+1)
	:endif
:endif

:if:<storage.get(int,RemoveItemAmount)>>0
	@storage.set(str,AddItemOptions,<system.token(PC_Decorations_Remove)>,<storage.get(str,AddItemOptions)>)
	@storage.set(int,AddItemAmount,<storage.get(int,AddItemAmount)>+1)
:endif

@Text.Show(<system.token(Generic_PC_Decoration_Start)>)
@options.setcancelindex(<storage.get(int,AddItemAmount)>)
@options.show(<storage.get(str,AddItemOptions)>)
	:when:<system.token(PC_Decorations_Item_SniffCat)>
		@Entity.SetVisible(0,1)
		@Entity.Warp(4,~,0,~)
		@Text.Show(<system.token(PC_Decorations_Placed)>)
		@Register.Register(DecorationPlaced_SniffCat)
		@script.start(Computer_Decoration)
	:when:<system.token(PC_Decorations_Item_DJSet)>
		@Text.Show(<system.token(PC_Decorations_Placed)>)
		@Entity.SetVisible(1,1)
		@Entity.SetCollision(5,1)
		@Entity.Warp(5,~,0,~)
		@Register.Register(DecorationPlaced_DJSet)
		@script.start(Computer_Decoration)
	:when:<system.token(PC_Decorations_Item_LeagueTrophy)>
		@Entity.SetVisible(2,1)
		@Entity.Warp(6,~,0,~)
		@Text.Show(<system.token(PC_Decorations_Placed)>)
		@Register.Register(DecorationPlaced_LeagueTrophy)
		@script.start(Computer_Decoration)
	:when:<system.token(PC_Decorations_Item_DexDiploma)>
		@Entity.SetVisible(3,1)
		@Entity.Warp(7,~,0,~)
		@Text.Show(<system.token(PC_Decorations_Placed)>)
		@Register.Register(DecorationPlaced_DexDiploma)
		@script.start(Computer_Decoration)
	:when:<system.token(PC_Decorations_Item_SurfingPikachuDoll)>
		@Entity.SetVisible(8,1)
		@Entity.Warp(9,~,0,~)
		@Text.Show(<system.token(PC_Decorations_Placed)>)
		@Register.Register(DecorationPlaced_SurfingPikachuDoll)
		@script.start(Computer_Decoration)
	:when:<system.token(PC_Decorations_Item_PottedPlant)>
		@Entity.SetVisible(10,1)
		@Entity.SetCollision(10,1)
		@Text.Show(<system.token(PC_Decorations_Placed)>)
		@Register.Register(DecorationPlaced_PottedPlant)
		@script.start(Computer_Decoration)
	:when:<system.token(PC_Decorations_Item_SquirtleDoll)>
		@Entity.SetVisible(11,1)
		@Entity.Warp(12,~,0,~)
		@Text.Show(<system.token(PC_Decorations_Placed)>)
		@Register.Register(DecorationPlaced_SquirtleDoll)
		@script.start(Computer_Decoration)
	:when:<system.token(PC_Decorations_Remove)>
		@Text.Show(<system.token(PC_Decorations_RemoveQuestion)>)
		@options.setcancelindex(<storage.get(int,RemoveItemAmount)>)
		@options.show(<storage.get(str,RemoveItemOptions)>)
			:when:<system.token(PC_Decorations_Item_SniffCat)>
				@Entity.SetVisible(0,0)
				@Entity.Warp(4,~,-1,~)
				@Text.Show(<system.token(PC_Decorations_Removed)>)
				@Register.Unregister(DecorationPlaced_SniffCat)
				@script.start(Computer_Decoration)
			:when:<system.token(PC_Decorations_Item_DJSet)>
				@Entity.SetVisible(1,0)
				@Entity.SetCollision(5,0)
				@Entity.Warp(5,~,-1,~)
				@Text.Show(<system.token(PC_Decorations_Removed)>)
				@Register.Unregister(DecorationPlaced_DJSet)
				@script.start(Computer_Decoration)
			:when:<system.token(PC_Decorations_Item_LeagueTrophy)>
				@Entity.SetVisible(2,0)
				@Entity.Warp(6,~,-1,~)
				@Text.Show(<system.token(PC_Decorations_Removed)>)
				@Register.Unregister(DecorationPlaced_LeagueTrophy)
				@script.start(Computer_Decoration)
			:when:<system.token(PC_Decorations_Item_DexDiploma)>
				@Entity.SetVisible(3,0)
				@Entity.Warp(7,~,-1,~)
				@Text.Show(<system.token(PC_Decorations_Removed)>)
				@Register.Unregister(DecorationPlaced_DexDiploma)
				@script.start(Computer_Decoration)
			:when:<system.token(PC_Decorations_Item_SurfingPikachuDoll)>
				@Entity.SetVisible(8,0)
				@Entity.Warp(9,~,-1,~)
				@Text.Show(<system.token(PC_Decorations_Removed)>)
				@Register.Unregister(DecorationPlaced_SurfingPikachuDoll)
				@script.start(Computer_Decoration)
			:when:<system.token(PC_Decorations_Item_PottedPlant)>
				@Entity.SetVisible(10,0)
				@Entity.SetCollision(10,0)
				@Text.Show(<system.token(PC_Decorations_Removed)>)
				@Register.Unregister(DecorationPlaced_PottedPlant)
				@script.start(Computer_Decoration)
			:when:<system.token(PC_Decorations_Item_SquirtleDoll)>
				@Entity.SetVisible(11,0)
				@Entity.Warp(12,~,-1,~)
				@Text.Show(<system.token(PC_Decorations_Removed)>)
				@Register.Unregister(DecorationPlaced_SquirtleDoll)
				@script.start(Computer_Decoration)
			:when:<system.token(global_back)>
				@script.start(Computer_Decoration)
		:endwhen
	:when:<system.token(global_back)>
		@Text.Show(<system.token(Generic_PC_ChooseHost)>)
		@script.start(Computer_Choose)
	:endwhen
:end
