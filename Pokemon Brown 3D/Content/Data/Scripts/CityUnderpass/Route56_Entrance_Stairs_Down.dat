version=2

:if:<Player.Facing>=0
	@Player.Warp(Routes\CityUnderpass.dat,0,0.1,0,3)
:else
	:if:<Player.Facing>=1
		@Player.Warp(Routes\CityUnderpass.dat,0,0.1,0,2)
	:else
		:if:<Player.Facing>=2
			@Player.Warp(Routes\CityUnderpass.dat,0,0.1,0,1)
		:else
			@Player.Warp(Routes\CityUnderpass.dat,0,0.1,0,0)
		:endif
	:endif
:endif
@Level.Update
:if:<Level.Riding>=true
	@Player.SetSpeed(2)
:else
	:if:<Player.IsRunning>=true
		@Player.SetSpeed(1.5)
	:endif
:endif
@Player.Move(1)
:end