version=2

:if:<Player.ThirdPerson>=false
	@Camera.SetToPlayerFacing
:endif
:if:<Player.Facing>=0
	@Player.Warp(Routes\56\CityUnderpass_Entrance.dat,4,0.1,3,1)
:else
	:if:<Player.Facing>=1
		@Player.Warp(Routes\56\CityUnderpass_Entrance.dat,4,0.1,3,0)
	:else
		:if:<Player.Facing>=2
			@Player.Warp(Routes\56\CityUnderpass_Entrance.dat,4,0.1,3,3)
		:else
			@Player.Warp(Routes\56\CityUnderpass_Entrance.dat,4,0.1,3,2)
		:endif
	:endif
:endif
@Level.Update
:if:<Level.Riding>=true
	@Player.SetSpeed(2)
:else
	:if:<Player.IsRunning>=true
		@Player.SetSpeed(1.5)
	:endif
:endif
@Player.Move(1)
:end
