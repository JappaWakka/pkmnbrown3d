version=2

:if:<player.position(z)>=37 <and> <Register.Registered(guard8)>=False
	@NPC.Turn(0,1)
	@Player.TurnTo(3)
	@Text.Show(You can pass here only if~you have the Star Badge!)
	
	:if:<Register.Registered(starbadge_obtain)>=True
		:if:<Register.Registered(guard8)>=False
			@Sound.Play(Success_Small)
			@Register.Register(guard8)
		:endif
		@Text.Show(Oh! That is the Star Badge!~OK then! Please, go right~ahead!)
		
	:endif
	
	:if:<Register.Registered(starbadge_obtain)>=False
		@Text.Show(You don't have the Star Badge~yet!*You have to have it to get~to the Pok√©mon League!)
		@player.turnto(2)
		@player.move(1)
	:endif
:endif
:end
