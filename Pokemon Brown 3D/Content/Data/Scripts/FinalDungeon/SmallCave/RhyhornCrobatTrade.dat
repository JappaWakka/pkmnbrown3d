version=2

:if:<Register.Registered(crobattrade)>=false
@Text.Show(<system.token(FinalDungeon_SmallCave_CrobatTrade_Question)>)
	@options.show(<system.token(global_yes)>,<system.token(global_no)>)
	:when:<system.token(global_yes)>
		@pokemon.npctrade(111|169||-1|||10396|Lothar|5||<system.token(FinalDungeon_SmallCave_CrobatTrade_CatchLocation)>|<system.token(FinalDungeon_SmallCave_CrobatTrade_CatchMethod)>|Keese|<system.token(FinalDungeon_SmallCave_CrobatTrade_TradeSuccess)>|<system.token(FinalDungeon_SmallCave_CrobatTrade_TradeFail)>|crobattrade)
		:end
	
	:when:<system.token(global_no)>
		@Text.Show(<system.token(FinalDungeon_SmallCave_CrobatTrade_Answer_No)>)
		:end
	:endwhen
:else
	@Text.Show(<system.token(FinalDungeon_SmallCave_CrobatTrade_AfterTrade)>)
:endif	
:end