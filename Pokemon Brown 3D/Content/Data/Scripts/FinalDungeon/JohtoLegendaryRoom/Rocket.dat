version=2

:if:<Register.Registered(JohtoLegendaryRoom)>=False
	@Text.Show(<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_BeforeGiveLegendary)>)
	:if:<system.random(0,<math.floor(<pokemon.mastershinyrate>)>)>=0
		@storage.set(int,PokeShiny,1)
	:else
		@storage.set(int,PokeShiny,0)
	:endif
	@Sound.Play(Success,1)
	:if:<Register.Registered(Lugia)>=True
		@Text.Show(<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_ReceivedLugia)>)
		@pokemon.add(249,50,<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_Pokemon_CatchMethod)>,5,<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_Pokemon_CatchLocation)>,0,<player.name>,0,<storage.get(int,PokeShiny)>)
	:else
		@Text.Show(<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_ReceivedHoOh)>)
		@pokemon.add(250,50,<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_Pokemon_CatchMethod)>,5,<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_Pokemon_CatchLocation)>,0,<player.name>,0,<storage.get(int,PokeShiny)>)
	:endif
	@pokemon.rename(<math.int(<pokemon.count>-1)>)
	:if:<pokemon.count>>6
		@Text.Show(<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_PokemonToPC)>)
		@pokemon.sendtostorage(<math.int(<pokemon.count>-1)>)
	:endif
	@Text.Show(<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_AfterGiveLegendary)>)
:else
	@Text.Show(<system.token(FinalDungeon_JohtoLegendaryRoom_Rocket_TalkAgain)>)
:endif
:end