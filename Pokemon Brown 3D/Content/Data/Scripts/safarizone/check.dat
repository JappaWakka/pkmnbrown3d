version=2

:if:<Register.Registered(safaritimer)>=False
	@Text.Show(Ding! Your time's up!)
	@screen.fadeout
	@player.warp(Eagulou\safari\gate.dat,1,0.1,4)
	@player.turnto(1)
	@level.update
	@screen.fadein
	@player.move(2)
	@player.turnto(2)
	:if:<inventory.countitem(181)>>0
		@Text.Show(You must now return your~Safari balls.)
		@item.remove(181,<inventory.countitem(181)>)
	:endif
	@Text.Show(Thank you. Come again!)
	:end
:else
	:end
:endif
:end