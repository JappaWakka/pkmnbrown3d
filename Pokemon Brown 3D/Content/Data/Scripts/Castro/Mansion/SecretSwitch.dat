version=2
@Text.Show(<system.token(Castro_Mansion_SecretSwitch_Question)>)
:if:<Register.Registered(mansionlock)>=false
	@options.show(<system.token(global_yes)>,<system.token(global_no)>)
	:when:<system.token(global_no)>
		@Text.Show(<system.token(Castro_Mansion_SecretSwitch_Answer_No)>)
		:end
	:when:<system.token(global_yes)>
		@Text.Show(<system.token(Castro_Mansion_SecretSwitch_Answer_Yes)>)
		@Register.Register(mansionlock)
		@level.reload
		:end
	:endwhen
:else:
	@Text.Show(<system.token(Castro_Mansion_SecretSwitch_Question)>)
	@options.show(<system.token(global_yes)>,<system.token(global_no)>)
	:when:<system.token(global_no)>
		@Text.Show(<system.token(Castro_Mansion_SecretSwitch_Answer_No)>)
		:end
	:when:<system.token(global_yes)>
		@Text.Show(<system.token(Castro_Mansion_SecretSwitch_Answer_Yes)>)
		@register.unregister(mansionlock)
		@level.reload
		:end
	:endwhen
:endif
:end