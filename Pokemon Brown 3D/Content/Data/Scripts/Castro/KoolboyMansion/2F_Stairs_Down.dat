version=2

:if:<Player.Facing>=0
	@Player.Warp(Castro\KoolboyMansion\1F.dat,5,0.1,8,1)
:else
	:if:<Player.Facing>=1
		@Player.Warp(Castro\KoolboyMansion\1F.dat,5,0.1,8,0)
	:else
		:if:<Player.Facing>=2
			@Player.Warp(Castro\KoolboyMansion\1F.dat,5,0.1,8,3)
		:else
			@Player.Warp(Castro\KoolboyMansion\1F.dat,5,0.1,8,2)
		:endif
	:endif
:endif
@Level.Update
:if:<Level.Riding>=true
	@Player.SetSpeed(2)
:else
	:if:<Player.IsRunning>=true
		@Player.SetSpeed(1.5)
	:endif
:endif
@Player.Move(1)
:end