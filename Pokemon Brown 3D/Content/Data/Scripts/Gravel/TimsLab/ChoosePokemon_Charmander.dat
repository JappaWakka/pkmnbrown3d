version=2
:if:<Register.Registered(pokemongot)>=False
	@screen.showpokemon(4,<register.value(Shiny_Starter_Charmander)>,1)
	@Text.Show(<system.token(Gravel_TimsLab_Choose_Charmander)>)
	@options.show(<system.token(global_yes)>,<system.token(global_no)>)
		:when:<system.token(global_yes)>
			@NPC.remove(1)
			@NPC.register(TimsLab.dat|1|remove)
			@Register.Register(pokemongot)
			@Register.Register(Starter_Charmander)
			@register.unregister(tim1)
			@register.unregister(mura1)
			@Register.Register(tim2)
			@Register.Register(mura2)
			@Text.Show(<system.token(Gravel_TimsLab_Choose_Confirm)>)
			@pokemon.add(4,5,[system.token(Gravel_TimsLab_StarterPokemon_CatchMethod)],5,[system.token(Gravel_TimsLab_StarterPokemon_CatchLocation)],0,<player.name>,0,<register.value(Shiny_Starter_Charmander)>)
			@overworldpokemon.hide
			@sound.play(success,1)
			@Text.Show(<system.token(Gravel_TimsLab_Receive_Charmander)>)
			@pokemon.rename(0)
			:if:<player.position>=6,0,3
				@NPC.turn(4,2)
				@NPC.move(4,2)
				@NPC.turn(4,3)
				@NPC.move(4,3)
				@NPC.turn(4,0)
				@NPC.move(4,1)
				@Text.Show(<system.token(Gravel_TimsLab_Mura_ChoosePokemon)>)
				@NPC.remove(2)
				@Text.Show(<system.token(Gravel_TimsLab_Mura_Receive_Squirtle)>)
			:endif
		:when:<system.token(global_no)>
			:end
		:endwhen
:else
	@Text.Show(<system.token(Gravel_TimsLab_PokeballAfterChoose)>)
:endif
:end