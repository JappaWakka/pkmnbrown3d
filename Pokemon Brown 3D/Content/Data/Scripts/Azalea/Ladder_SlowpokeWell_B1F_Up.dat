version=2
:if:<Player.ThirdPerson>=false
	@Camera.SetToPlayerFacing
:endif
:if:<Player.Facing>=0
	@Player.Warp(Azalea\SlowpokeWell\1F.dat,5,1.1,9,0)
:else
	:if:<Player.Facing>=1
		@Player.Warp(Azalea\SlowpokeWell\1F.dat,5,1.1,9,0)
	:else
		:if:<Player.Facing>=2
			@Player.Warp(Azalea\SlowpokeWell\1F.dat,5,1.1,9,0)
		:else
			@Player.Warp(Azalea\SlowpokeWell\1F.dat,5,1.1,9,0)
		:endif
	:endif
:endif
