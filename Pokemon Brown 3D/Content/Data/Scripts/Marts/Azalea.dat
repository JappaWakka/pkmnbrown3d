version=2
@Text.Show(<system.token(Azalea_Main_Clerk_Question)>)
@options.show(<system.token(Generic_PokeMart_Store)>,<system.token(Generic_PokeMart_Quit)>)
	:when:<system.token(Generic_PokeMart_Store)>
		@Text.Show(<system.token(Azalea_Main_Clerk_Store)>)
		@screen.Trade({5|-1|-1}{gm79|-1|-1},True,True)
		@Text.Show(<system.token(Azalea_Main_Clerk_Quit)>)
	:when:<system.token(Generic_PokeMart_Quit)>
		@Text.Show(<system.token(Azalea_Main_Clerk_Quit)>)
:endwhen
:end